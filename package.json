{
  "name": "krok",
  "version": "0.0.2",
  "private": true,
  "author": "Andrii Pichkorskiy",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "pod_ios": "cd ./ios && pod install",
    "bundle:android": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output dist/index.android.bundle --sourcemap-output dist/index.android.bundle.map",
    "bundle:ios": "npx react-native bundle --platform ios --dev false --entry-file index.js --bundle-output dist/index.ios.bundle --sourcemap-output dist/index.ios.bundle.map",
    "analyze:source-map": "npx source-map-explorer dist/index.android.bundle dist/index.android.bundle.map",
    "analyze:visualizer": "npx react-native-bundle-visualizer",
    "analyze:both": "npm run analyze:source-map && npm run analyze:visualizer"
  },
  "dependencies": {
    "@react-native-masked-view/masked-view": "^0.3.2",
    "@react-native-vector-icons/fontawesome5": "^12.2.0",
    "@react-native/new-app-screen": "0.81.1",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/drawer": "^7.5.8",
    "@react-navigation/stack": "^7.4.8",
    "@reduxjs/toolkit": "^2.9.0",
    "axios": "^1.11.0",
    "react": "19.1.0",
    "react-native": "0.81.1",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-reanimated": "^4.1.0",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.16.0",
    "react-native-svg": "^15.13.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-worklets": "^0.5.1",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native-community/cli-plugin-metro": "^12.3.6",
    "@react-native/babel-preset": "0.81.1",
    "@react-native/eslint-config": "0.81.1",
    "@react-native/metro-config": "0.81.1",
    "@react-native/typescript-config": "0.81.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "metro-visualizer": "^0.56.4",
    "prettier": "2.8.8",
    "react-native-bundle-visualizer": "^3.1.3",
    "react-native-dotenv": "^3.4.11",
    "react-test-renderer": "19.1.0",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  }
}
